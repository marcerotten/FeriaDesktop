<Window x:Class="FeriaDesktop.View.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeriaDesktop.View"
        mc:Ignorable="d"
        Title="FERIA VIRTUAL" Height="450" Width="800">

    <Grid>
        <ContentControl Content="{Binding SelectedViewModel}"/>

        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="CLIENTES" Height="47" Width="109" Command="{Binding GetUsersCommand}" CommandParameter="Users" RenderTransformOrigin="-1.876,0.649"/>
            <Button Content="CONTRATOS" Height="47" Width="109" Command="{Binding GetContractsCommand}" CommandParameter="Contracts" RenderTransformOrigin="-1.876,0.649"/>

        </StackPanel>
    </Grid>
</Window>
