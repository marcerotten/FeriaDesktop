<Window x:Class="FeriaDesktop.View.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeriaDesktop.View"
        mc:Ignorable="d"
        Title="FERIA VIRTUAL" Height="450" Width="800">

    <Grid>
        <ContentControl Content="{Binding SelectedViewModel}"/>

        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="CLIENTES" HorizontalAlignment="Left" Margin="25,108,0,0" VerticalAlignment="Top" Height="47" Width="109" Command="{Binding GetUsersCommand}" CommandParameter="Users"/>
        </StackPanel>
    </Grid>
</Window>
